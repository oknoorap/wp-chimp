{
  "name": "wp-chimp",
  "version": "0.1.0-alpha.1",
  "description": "Lightweight MailChimp form for WordPress",
  "main": "wp-chimp.php",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfirdaus/wp-chimp.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "mailchimp",
    "form",
    "subscription"
  ],
  "author": "Thoriq Firdaus",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/tfirdaus/wp-chimp/issues"
  },
  "homepage": "https://github.com/tfirdaus/wp-chimp#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-eslint": "^4.0.2",
    "gulp-readme-to-markdown": "^0.2.1",
    "gulp-sass": "^3.2.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglifyes": "^0.1.3",
    "gulp-wp-pot": "^2.3.1",
    "gulp-zip": "^4.1.0",
    "merge-stream": "^1.0.1",
    "stylelint": "^9.3.0",
    "stylelint-config-wordpress": "^13.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "camelcase-keys": "^4.2.0",
    "redom": "^3.12.4",
    "snakecase-keys": "^1.1.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "wordpress",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": [
    "*.min.js",
    "*.js",
    "node_modules/*",
    "vendor/*"
  ],
  "stylelint": {
    "extends": "stylelint-config-wordpress"
  },
  "scripts": {
    "start": "npm install -g http-server && http-server -p 3000",
    "precommit": "dev-kit/bin/ci-precommit",
    "local:run": "dev-kit/bin/run",
    "local:up": "dev-kit/bin/up -d wordpress",
    "local:down": "dev-kit/bin/down",
    "local:start": "dev-kit/bin/start",
    "local:restart": "dev-kit/bin/start",
    "local:destroy": "dev-kit/bin/destroy",
    "local:ssh": "dev-kit/bin/ssh",
    "local:ssh-node": "dev-kit/bin/ssh node",
    "test:phpunit": "dev-kit/bin/phpunit",
    "test:phpcs": "dev-kit/bin/phpcs",
    "test:eslint": "dev-kit/bin/eslint",
    "test:stylelint": "./node_modules/.bin/stylelint 'assets/css/*.*'"
  }
}
