{
  "name": "wp-chimp",
  "version": "0.2.0",
  "description": "Lean MailChimp subscription form plugin for WordPress",
  "main": "wp-chimp.php",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfirdaus/wp-chimp.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "mailchimp",
    "form",
    "subscription"
  ],
  "author": "Thoriq Firdaus",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/tfirdaus/wp-chimp/issues"
  },
  "homepage": "https://github.com/tfirdaus/wp-chimp#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "event-stream": "^3.3.4",
    "grunt": "^1.0.3",
    "grunt-wp-deploy": "^1.3.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-eslint": "^4.0.2",
    "gulp-sass": "^4.0.1",
    "gulp-shell": "^0.6.5",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglifyes": "^0.1.3",
    "gulp-wp-pot": "^2.3.1",
    "gulp-zip": "^4.1.0",
    "husky": "^0.14.3",
    "merge-stream": "^1.0.1",
    "stylelint": "^9.3.0",
    "stylelint-config-wordpress": "^13.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "camelcase-keys": "^4.2.0",
    "redom": "^3.12.4",
    "snakecase-keys": "^1.2.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "wordpress",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": [
    "*.min.js",
    "*.map",
    "node_modules/*",
    "vendor/*"
  ],
  "stylelint": {
    "extends": "stylelint-config-wordpress"
  },
  "scripts": {
    "local:up": "bash localhost/up.sh -d wordpress",
    "local:down": "bash localhost/down.sh",
    "local:start": "bash localhost/start.sh",
    "local:restart": "bash localhost/start.sh",
    "local:destroy": "bash localhost/destroy.sh",
    "local:ssh": "bash localhost/ssh.sh",
    "local:export-db": "bash localhost/mysql-export.sh",
    "local:import-db": "bash localhost/mysql-import.sh",
    "precommit": "bash pipelines/pre-commit.sh",
    "test": "bash pipelines/tests.sh"
  }
}
